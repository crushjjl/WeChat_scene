html,body,ul,li,dl,ol,dt,dd,p,h1,h2,h3,h4,h5,h6,img,span{
	margin: 0;
	padding: 0;
}
img{
	border: 0;
}
a{
	text-decoration: none;
}
ul,li{
	list-style: none;
}
html{
	font-size: 15.625vw;
}
html,body{
	overflow: hidden;
	height: 100%;
}
.swiper-wrapper{
	height: 100vh;
	background: lightblue;
}
.swiper-wrapper p img{
	width: 100%;
}
.swiper-slide{
	padding-top: 0;
	overflow: hidden;
}
